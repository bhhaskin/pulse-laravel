{
    "name": "bhhaskin/pulse-laravel",
    "description": "Laravel integration package that exposes Pulse JS API endpoints.",
    "type": "library",
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "Bryan Haskin",
            "email": "bhhaskin@bitsofsimplicity.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Bhhaskin\\Pulse\\": "src/",
            "Bhhaskin\\Pulse\\Database\\": "database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bhhaskin\\Pulse\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bhhaskin\\Pulse\\PulseServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
